{
  "name": "nexium-rpg-bot",
  "version": "1.0.0",
  "description": "Nexium RPG: The Eidolon Protocol - A sophisticated Discord RPG bot",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "deploy": "npm run build && npm run deploy-commands",
    "deploy-commands": "node dist/deploy-commands.js",
    "seed": "tsx src/database/seed.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "rpg",
    "gaming"
  ],
  "author": "Nexium RPG Team",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.19.3",
    "discraft": "^1.7.8",
    "dotenv": "^16.4.5",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/pg": "^8.15.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
